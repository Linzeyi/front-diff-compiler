{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "recurseDepth": 10,
  "source": {
    "include": [
      "./src"
    ],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "./jsdoc-vue"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./JSDOC",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}